(defparameter *output* 0)


(defun tictactoe (list-board)
  (setq 
    *output* 
    (second 
      (minimax-alphabeta
        (create-board list-board) 5 most-negative-fixnum most-positive-fixnum T
      )
    )
  )
)



(defparameter list-board 
  '(( NIL NIL NIL NIL)
    ( NIL NIL  X  NIL)
    ( NIL X O NIL)
    ( NIL NIL NIL NIL))  
)

(print (get-moves (create-board list-board) T))

(tictactoe list-board)


(print *output*)